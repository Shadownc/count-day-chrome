{
  "manifest_version": 3,
  "name": "日期倒计时",
  "description": "显示节假日倒计时和日期进度",
  "version": "1.0",
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "popup/popup.html"
  },
  "web_accessible_resources": [{
    "resources": ["icons/*"],
    "matches": ["<all_urls>"]
  }]
}